from turtle import Turtle,Screen

maano_turtle = Turtle()
maano_turtle.color("green")
maano_turtle.shape("arrow")
position = 0
while (position < 120):
    maano_turtle.forward(10)
    maano_turtle.penup()
    maano_turtle.forward(10)
    maano_turtle.pendown()
    position += 10

screen=Screen()
screen.exitonclick()
